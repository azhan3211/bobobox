<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bobobox.bobobox">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"></activity>
        <activity android:name=".SignIn"></activity>
        <activity android:name=".SignUp"></activity>
        <activity android:name=".WelcomeBobobox"></activity>
        <activity android:name=".BookingDate"></activity>
        <activity android:name=".BookingHour"></activity>
        <activity android:name=".BookingCalendar"></activity>
        <activity android:name=".SearchResult"></activity>
        <activity android:name=".DetailRoom"></activity>
        <activity android:name=".BookingConfirmationStep1"></activity>
        <activity android:name=".BookingConfirmationStep2"></activity>
        <activity android:name=".BookingConfirmationStep3"></activity>
        <activity android:name=".ActiveBooking"></activity>
        <activity android:name=".HistoryBooking"></activity>
        <activity android:name=".Validation"></activity>
        <meta-data android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id"/>
    </application>

</manifest>